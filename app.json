{
  "expo": {
    "name": "plantmanager",
    "slug": "plantmanager",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "radial-gradient(107.35% 50% at 50% 50%, #FFFFFF 0%, #EDFCF4 100%);"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "radial-gradient(107.35% 50% at 50% 50%, #FFFFFF 0%, #EDFCF4 100%);"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
